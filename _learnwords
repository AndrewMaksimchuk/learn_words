#compdef learnwords
# Add competion for shell(zsh)


local -a subcmds
subcmds=(
    'run:Show notification' 
    'update:Add new words to dictionary' 
    'open:Run gui' 
    'add:Open editor with "words" file' 
    'build:Build GUI application'
    'check:Check if word exist in dictionary'
)
_describe 'command' subcmds